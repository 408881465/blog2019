    "@angular/router": "8.0.2",
    "@ionic/angular": "4.5.0",
    "cettia-client": "1.1.0",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "protobufjs": "6.8.8",
    "uuid": "3.3.2",
    "zone.js": "0.9.1"
  },
