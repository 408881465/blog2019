{
  "name": "buildinfo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "serve-dist": "ws --hostname localhost -d dist/buildinfo -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "prebuild-prod": "node ./build-info-env.js",
    "build-prod": "ng build --prod",
    "postbuild-prod": "node ./git-tag.js",
    "release-patch": "npm --no-git-tag-version version patch && npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "8.0.1",
    "@angular/compiler": "8.0.1",
    "@angular/core": "8.0.1",
    "@angular/platform-browser": "8.0.1",
    "@angular/platform-browser-dynamic": "8.0.1",
    "@angular/router": "8.0.1",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.3",
    "@angular/cli": "8.0.3",
    "@angular/compiler-cli": "8.0.1",
    "@angular/language-service": "8.0.1",
    "@types/node": "12.0.8",
    "codelyzer": "5.1.0",
    "local-web-server": "3.0.1",
    "replace-in-file": "4.1.0",
    "simple-git": "^1.113.0",
    "tslint": "5.17.0",
    "typescript": "3.4.5"
  }
}
